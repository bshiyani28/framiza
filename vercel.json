{
  "builds": [
      {
          "src": "myproject/wsgi.py",
          "use": "@vercel/python",
          "config": {
              "maxLambdaSize": "15mb",
              "runtime": "python3.10",
              "buildCommand" : "vercel_postbuild.sh"
          }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "myproject/wsgi.py"
      },
      {
        "src": "static/(.*)",
        "dest": "/static/$1"
      }
  ]
}