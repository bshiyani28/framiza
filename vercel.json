{
    "build": {
        "commands": [
            "pip install -r requirements.txt",
            "python manage.py makemigrations",
            "python manage.py migrate"
        ]
    },
    "builds": [
        {
            "src": "myproject/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "myproject/wsgi.py"
        }
    ]
}